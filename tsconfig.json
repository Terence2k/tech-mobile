{
  "compilerOptions": {
    "target": "esnext",
    "module": "esnext",
    "moduleResolution": "node",
    "importHelpers": true,
    "noImplicitAny": false,
    "jsx": "react",
    "esModuleInterop": true,
    "sourceMap": true,
    "baseUrl": ".",
    "strict": true,
    "paths": {
      "@/*": ["src/*"],
      "@tmp/*": ["src/pages/.umi/*"],
      "actions": ["src/actions/index"],
      "hooks": ["src/hooks/index"]
    },
    "allowSyntheticDefaultImports": true
  }
}
